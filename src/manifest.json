{
  "manifest_version": 2,
  "name": "Whitehouse.gov Archive Helper",
  "short_name": "WH.gov Redirect",
  "version": "1.0.4",
  "description": "Redirects to the archived whitehouse.gov if the current whitehouse.gov 404s",
  "author": "David Chang",
  "homepage_url": "https://github.com/liyanchang/whitehouse.gov-redirect",
  "minimum_chrome_version": "32",
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },

  "permissions": [
      "webRequest",
      "*://*.whitehouse.gov/*"
  ],
  "background": {
      "scripts": ["main.js"]
  }
}
